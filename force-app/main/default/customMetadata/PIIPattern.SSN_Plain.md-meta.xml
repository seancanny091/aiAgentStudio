<?xml version="1.0" encoding="UTF-8"?>
<CustomMetadata xmlns="http://soap.sforce.com/2006/04/metadata" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <label>SSN (Plain Format)</label>
    <protected>false</protected>
    <values>
        <field>Category__c</field>
        <value xsi:type="xsd:string">Identity</value>
    </values>
    <values>
        <field>Description__c</field>
        <value xsi:type="xsd:string">Detects US Social Security Numbers as 9 consecutive digits preceded by SSN/social security/taxpayer ID keyword anchor. Validates against known invalid patterns. Lower priority than dashed format.</value>
    </values>
    <values>
        <field>IsActive__c</field>
        <value xsi:type="xsd:boolean">true</value>
    </values>
    <values>
        <field>MaskFormat__c</field>
        <value xsi:type="xsd:string">[SSN:{index}]</value>
    </values>
    <values>
        <field>PatternRegex__c</field>
        <value xsi:type="xsd:string">(?i)\b(?:ssn|social[\s\-]*security(?:\s*(?:number|no\.?|#))?|taxpayer[\s\-]*id)\b[:\s#]*((?!000|666|9\d{2})\d{3}[\s\-]?(?!00)\d{2}[\s\-]?(?!0000)\d{4})\b</value>
    </values>
    <values>
        <field>Priority__c</field>
        <value xsi:type="xsd:double">15.0</value>
    </values>
    <values>
        <field>RequiresValidation__c</field>
        <value xsi:type="xsd:boolean">true</value>
    </values>
    <values>
        <field>ValidatorType__c</field>
        <value xsi:type="xsd:string">SSN_INVALID</value>
    </values>
</CustomMetadata>
